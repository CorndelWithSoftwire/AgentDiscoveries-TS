{
  "name": "agent-discoveries",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "install:backend": "npm --prefix ./agent-discoveries-backend install",
    "install:frontend": "npm --prefix ./agent-discoveries-frontend install",
    "postinstall": "npm run install:backend && npm run install:frontend",
    "prepare": "husky install",
    "test": "jest",
    "dev:backend": "npm run --prefix ./agent-discoveries-backend dev",
    "dev:frontend": "npm run --prefix ./agent-discoveries-frontend dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
